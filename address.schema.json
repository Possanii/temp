{
   "$id":"https://example.com/address.schema.json",
   "description":"An address similar to http://microformats.org/wiki/h-card",
   "type":"object",
   "properties":{
      "postOfficeBox":{
         "type":"string"
      },
      "extendedAddress":{
         "type":"string"
      },
      "streetAddress":{
         "type":"string"
      },
      "locality":{
         "type":"string"
      },
      "region":{
         "type":"string"
      },
      "postalCode":{
         "type":"string"
      },
      "countryName":{
         "type":"string"
      }
   },
   "required":[
      "locality",
      "region",
      "countryName"
   ],
   "dependentRequired":{
      "postOfficeBox":[
         "streetAddress"
      ],
      "extendedAddress":[
         "streetAddress"
      ]
   }
}
